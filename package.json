{
  "pnpm": {
    "overrides": {
      "metro": "0.80.8",
      "metro-config": "0.80.8",
      "metro-resolver": "0.80.8",
      "@expo/config-plugins": "8.0.6"
    }
  },
  "name": "eatsense-mobile",
  "version": "1.0.0",
  "description": "EatSense Mobile Client - React Native + Expo",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env -- expo start --tunnel --clear",
    "start:tunnel": "dotenv -e .env -- expo start --tunnel --clear",
    "start:lan": "dotenv -e .env -- expo start --lan --clear",
    "start:dev": "REACT_NATIVE_PACKAGER_HOSTNAME=172.20.10.2 dotenv -e .env -- expo start --lan --clear --reset-cache",
    "convert-logo": "./scripts/convert-logo.sh",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "detox test",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint && npm run type-check && npm run test",
    "setup": "pnpm install && pnpm run setup:api",
    "i18n:extract": "node scripts/i18n-extract.js",
    "i18n:verify": "node scripts/i18n-verify.js",
    "build": "nest build",
    "start:prod": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "eas:whoami": "pnpm dlx eas-cli@latest whoami",
    "eas:login": "pnpm dlx eas-cli@latest login",
    "eas:build:ios": "pnpm dlx eas-cli@latest build -p ios --profile production --non-interactive",
    "eas:build:android": "pnpm dlx eas-cli@latest build -p android --profile production --non-interactive",
    "eas:submit:ios": "pnpm dlx eas-cli@latest submit -p ios --profile production --latest --non-interactive"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-community/slider": "4.5.2",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "cli-spinners": "^3.3.0",
    "expo": "~51.0.39",
    "expo-asset": "~10.0.10",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.10",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "^15.0.3",
    "expo-media-library": "~16.0.5",
    "expo-notifications": "~0.28.19",
    "expo-router": "^3.5.14",
    "expo-secure-store": "^13.0.2",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "expo-apple-authentication": "~7.0.1",
    "expo-auth-session": "~6.0.1",
    "expo-crypto": "~13.0.1",
    "i18next": "^23.16.8",
    "moti": "^0.28.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "~4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "18.2.79",
    "@types/react-test-renderer": "^18.2.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eas-cli": "^16.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "prettier": "^3.2.5",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": "20.19.x"
  },
  "packageManager": "pnpm@10.19.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ]
}
