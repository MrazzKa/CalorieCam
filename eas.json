{
  "cli": {
    "version": ">= 3.21.0"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "staging"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "<apple-id>@example.com",
        "ascAppId": "<app-store-connect-app-id>",
        "appleTeamId": "<team-id>"
      }
    }
  }
}
