{
  "cli": {
    "version": ">= 3.21.0",
    "appVersionSource": "remote"
  },
  "build": {
    "simulator": {
      "distribution": "internal",
      "ios": { "simulator": true },
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_API_BASE_URL": "https://caloriecam-production.up.railway.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": { "resourceClass": "m-medium" },
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_API_BASE_URL": "https://caloriecam-production.up.railway.app"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "ios": { "resourceClass": "m-medium" },
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://caloriecam-production.up.railway.app"
      }
    }
  },
  "submit": { "production": { "ios": {} } }
}
